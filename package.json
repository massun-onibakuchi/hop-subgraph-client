{
  "name": "hop-subgraph-client",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npx rollup -c ",
    "dev": "npx rollup -c -w",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "format": "yarn format:ts && yarn format:graphql",
    "format:ts": "prettier --write '**/*.ts'",
    "format:graphql": "prettier --write '**/*.graphql'",
    "lint": "eslint ./src --ext .ts --max-warnings 0",
    "lint:fix": "eslint ./src --ext .ts --max-warnings 0 --fix",
    "gen:subgraph": "graphql-codegen --config src/codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-graphql-request": "^4.3.4",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/dotenv": "6.1.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "7.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-dts": "^4.1.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "author": "massun onibakuchi",
  "description": "ðŸ›  An JavaScript Subgraph Client for Hop Protocol",
  "keywords": [
    "hop protocol",
    "subgraph",
    "graphql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/massun-onibakuchi/hop-subgraph-client"
  },
  "bugs": {
    "url": "https://github.com/massun-onibakuchi/hop-subgraph-client/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
